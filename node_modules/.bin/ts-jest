#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/ts-jest@27.1.2_9364f2ad0b7b67f00a4d081c662c6871/node_modules/ts-jest/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/ts-jest@27.1.2_9364f2ad0b7b67f00a4d081c662c6871/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/node_modules:/home/arielschvartz/Dropbox/node_modules:/home/arielschvartz/node_modules:/home/node_modules:/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/ts-jest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/ts-jest@27.1.2_9364f2ad0b7b67f00a4d081c662c6871/node_modules/ts-jest/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/ts-jest@27.1.2_9364f2ad0b7b67f00a4d081c662c6871/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/node_modules:/home/arielschvartz/Dropbox/node_modules:/home/arielschvartz/node_modules:/home/node_modules:/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/ts-jest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
