#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/node_modules:/home/arielschvartz/Dropbox/node_modules:/home/arielschvartz/node_modules:/home/node_modules:/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules/jsesc/bin/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules/jsesc/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/node_modules:/home/arielschvartz/Dropbox/node_modules:/home/arielschvartz/node_modules:/home/node_modules:/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules/jsesc/bin/node_modules:/home/arielschvartz/Dropbox/Chocolate Soup/system/joiful-dynamodb/node_modules/.pnpm/node_modules/jsesc/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
